journal = {Clinical Psychology: Science and},
keywords = {affect phobia,desensitization,expo-,psychodynamic conflict,psychotherapy integra- tion,short-term dynamic psychotherapy.,sure},
pages = {82--97},
title = {{Assimilative Integration: Short-term Dynamic Psychotherapy for Treating Affect Phobias}},
volume = {8},
year = {2001}
}
