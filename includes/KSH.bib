issn = {08949085},
journal = {Journal of Rational-Emotive and Cognitive-Behavior Therapy},
keywords = {Adolescence,Assertiveness,Cognitive-behavioral,Social contexts,Structural equation modelling},
number = {2},
pages = {133--148},
publisher = {Springer US},
title = {{Towards a Cognitive-Behavioral Understanding of Assertiveness: Effects of Cognition and Distress on Different Expressions of Assertive Behavior}},
url = {https://doi.org/10.1007/s10942-018-0296-4},
volume = {37},
