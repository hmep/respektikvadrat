title = {{Internet-delivered acceptance-based behaviour therapy for generalized anxiety disorder: A randomized controlled trial}},
url = {http://ac.els-cdn.com.ezp.sub.su.se/S0005796715300723/1-s2.0-S0005796715300723-main.pdf?{\_}tid=7bcf6bfe-80e0-11e7-9efa-00000aacb362{\&}acdnat=1502708910{\_}1844d1b70d77249fc279a1e952b15803 http://linkinghub.elsevier.com/retrieve/pii/S0005796715300723},
