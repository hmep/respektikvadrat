title = {{The hippocampus as a predictive map}},
url = {http://www.biorxiv.org/content/biorxiv/early/2017/07/27/097170.full.pdf},
year = {2016}
}
@article{Wells2008,
abstract = {The theory and principles of Metacognitive therapy (MCT) are described and data supporting its effects are summarized. MCT does not advocate challenging of negative automatic thoughts or traditional schemas. It proposes the existence of a universal maladaptive thinking style that causes disorder and focuses on helping patients regulate their cognition more adaptively. It aims to reduce worry and rumination and alter problematic patterns of attention and coping. In doing so it targets underlying metacognition that controls thinking and helps patients develop new ways of consciously experiencing inner events. Data from treatment studies suggest that individual MCT techniques and full treatment are highly effective. Further randomized trials are clearly warranted.},
author = {Wells, Adrian},
