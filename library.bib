issn = {22121447},
journal = {Journal of Contextual Behavioral Science},
keywords = {Acceptance and Commitment Therapy,Experiential Avoidance,Psychological Inflexibility,Transdiagnostic},
month = {jul},
number = {3},
pages = {155--163},
title = {{Examining psychological inflexibility as a transdiagnostic process across psychological disorders}},
url = {http://ac.els-cdn.com.ezp.sub.su.se/S2212144714000556/1-s2.0-S2212144714000556-main.pdf?{\_}tid=1bbc7a9a-269f-11e7-a8db-00000aacb361{\&}acdnat=1492785226{\_}5de4d0fafda88ba0bd339afdfe394199 http://linkinghub.elsevier.com/retrieve/pii/S2212144714000556},
