issn = {2043-8087},
journal = {Journal of Experimental Psychopathology JEP},
keywords = {Social Anxiety Disorder,attention training technique,attentional control,metacognitive therapy,situational attentional refocusing},
pages = {608--618},
title = {{Metacognitive Therapy Applications in Social Anxiety Disorder: An Exploratory Study of the Individual and Combined Effects of the Attention Training Technique and Situational Attentional Refocusing}},
