issn = {1471-003X},
journal = {Nature Reviews Neuroscience},
keywords = {Bayes Theorem,Brain,Brain: pathology,Brain: physiopathology,Cognition,Cognition: physiology,Humans,Models,Neurological,Perception,Perception: physiology,Schizophrenia,Schizophrenia: physiopathology,Schizophrenic Psychology},
month = {jan},
