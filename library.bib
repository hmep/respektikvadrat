title = {{Intensive Short-Term Dynamic Psychotherapy for Severe Music Performance Anxiety}},
url = {https://medicine.dal.ca/content/dam/dalhousie/pdf/faculty/medicine/departments/department-sites/psychiatry/centre-emotions-health/performance{\_}anx{\_}pub.pdf}
