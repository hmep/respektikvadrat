file = {:home/tobias/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Uhl-Bien et al. - 2014 - Followership theory A review and research agenda.pdf:pdf},
isbn = {1048-9843},
issn = {10489843},
keywords = {Follower,Followership,Followership theory,Leadership,Leadership co-production},
number = {1},
pages = {83--104},
pmid = {457585},
title = {{Followership theory: A review and research agenda}},
url = {http://dx.doi.org/10.1016/j.leaqua.2013.11.007},
