@article{Parsons2012,
abstract = {Behavior analysts in human service agencies are commonly expected to train support staff as one of their job duties. Traditional staff training is usually didactic in nature and generally has not proven particularly effective. We describe an alternative, evidence-based approach for training performance skills to human service staff. The description includes a specific means of conducting a behavioral skills training session with a group of staff followed by on-the-job training requirements. A brief case demonstration then illustrates application of the training approach and its apparent effectiveness for training staff in two distinct skill sets: use of most-to-least prompting within teaching procedures and use of manual signs. Practical issues associated with applying evidence-based behavioral training are presented with a focus on providing training that is effective, efficient, and acceptable to staff trainees.},
author = {Parsons, Marsha B and Rollyson, Jeannia H. and Reid, Dennis H},
file = {:home/tobias/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Parsons, Rollyson, Reid - 2012 - Evidence-based staff training A guide for practitioners.pdf:pdf},
issn = {1998-1929},
journal = {Behavior analysis in practice},
keywords = {behavioral skills training,evidence-based practices,most-to-least,prompting,staff training},
number = {2},
pages = {2--11},
pmid = {23730462},
title = {{Evidence-based staff training: A guide for practitioners.}},
url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3592486/pdf/i1998-1929-5-2-2.pdf},
volume = {5},
year = {2012}
}
@incollection{SjobergA,
abstract = {{\"{A}}ven om medvetenheten om de ekonomiska aspekterna p{\aa} rekrytering och urval {\"{o}}kat de senaste {\aa}ren, {\"{a}}r kunskapen om effekterna av mer eller mindre valida urvalsmetoder p{\aa} produktiviteten fortfarande bristf{\"{a}}llig. Detta kapitel visar hur individuella skillnader i arbets-prestation kan {\aa}s{\"{a}}ttas ett ekonomiskt v{\"{a}}rde, och behandlar fr{\aa}gor som r{\"{o}}r de ekonomiska konsekvenserna av olika urvalsmetoder st{\"{a}}llda i relation till faktorer som olika typer av arbeten, antal s{\"{o}}kande och antal personer som beh{\"{o}}ver anst{\"{a}}llas. Genom exempel p{\aa} konkreta personalekonomiska ber{\"{a}}kningar p{\aa}visas vikten av att beakta validiteten i de test och metoder man anv{\"{a}}nder, hur man ska resonera n{\"{a}}r man v{\"{a}}ljer kombinationer av metoder, och i vilka situationer ett valitt urvalsf{\"{o}}rfarande har st{\"{o}}rst ekonomisk betydelse. Rekrytering Innan en diskussion om effekterna av graden av validitet i urvalsmetoder kan f{\"{o}}ras, b{\"{o}}r n{\aa}gra andra personalrelaterade begrepp belysas ur olika perspektiv, d{\aa} de har stora ekonomiska konsekvenser. Ett urval f{\"{o}}reg{\aa}s i allm{\"{a}}nhet av n{\aa}gon form av rekryterings-56 process, det vill s{\"{a}}ga de strategier och metoder man anv{\"{a}}nder f{\"{o}}r att v{\"{a}}rva s{\"{o}}kande till de aktuella tj{\"{a}}nsterna (ofta anv{\"{a}}nds rekrytering och urval synonymt, men {\"{a}}r egentligen tv{\aa} delar av det f{\"{o}}rfarande som syftar till att anst{\"{a}}lla ny personal). Det finns m{\aa}nga olika s{\"{a}}tt att rekrytera kandidater, och vilket som l{\"{a}}mpar sig b{\"{a}}st varierar med typen av arbete eller tj{\"{a}}nst. N{\"{a}}r man ska tills{\"{a}}tta komplexa befattningar, som till exempel en tj{\"{a}}nst som vd vid ett stort eller mellanstort f{\"{o}}retag, {\"{a}}r de formella kraven f{\"{o}}rmodligen t{\"{a}}mligen h{\"{o}}ga och v{\"{a}}lspecificerade. I s{\aa}dana fall kanske en searchkonsult {\"{a}}r det b{\"{a}}sta alternativet, speciellt om man vill vara diskret i sin rekrytering av den nya vd:n – en annons i dagspressen framst{\aa}r m{\aa}h{\"{a}}nda inte som ett aktuellt alternativ. Searchkonsulten kan d{\aa} anv{\"{a}}nda sitt n{\"{a}}tverk f{\"{o}}r att s{\"{o}}ka efter m{\"{o}}jliga kandidater och sedan presentera n{\aa}gra som f{\"{o}}retaget f{\aa}r v{\"{a}}lja mellan. F{\"{o}}rdelarna med denna metod {\"{a}}r diskretionen, tidsaspekten (att annonsera, inv{\"{a}}nta ans{\"{o}}kningar, granska dessa och s{\aa} vidare kan ta l{\aa}ng tid), och att den generar h{\"{o}}g s{\aa} kallad baskvot. Detta personal-ekonomiska begrepp anger den andel av de s{\"{o}}kande (oftast uttryckt i procent) som skulle bli framg{\aa}ngsrika medarbetare {\"{a}}ven om man inte till{\"{a}}mpade n{\aa}gon urvalsmetod alls. Baskvoten {\"{a}}r en teoretisk uppskattning som kan variera mellan branscher, konjunkturer, olika yrken och framf{\"{o}}r allt mellan olika rekryteringsf{\"{o}}rfaranden. I fallet med vd:n, d{\"{a}}r de formella kraven {\"{a}}r h{\"{o}}gt st{\"{a}}llda och en searchkonsult utifr{\aa}n sitt n{\"{a}}tverk identifierar l{\"{a}}mpliga kandidater, f{\aa}r man troligen en h{\"{o}}g baskvot – de kandidater som presenteras h{\aa}ller f{\"{o}}rmodligen mycket h{\"{o}}g kvalitet. Det finns emellertid {\"{a}}ven nackdelar med att anlita en searchkonsult; den allvarligaste {\"{a}}r f{\"{o}}rmodligen att f{\"{o}}retaget {\"{a}}r helt utl{\"{a}}mnat till konsultens n{\"{a}}tverk och d{\"{a}}rmed riskerar att g{\aa} miste om andra duktiga kandidater. Den ekonomiska aspekten p{\aa} denna typ av urvalsf{\"{o}}rfarande {\"{a}}r ocks{\aa} viktig. En searchkonsults arvode uppg{\aa}r i regel till cirka 35 {\%} av {\aa}rsl{\"{o}}nen f{\"{o}}r befattningen ifr{\aa}ga, och eftersom det n{\"{a}}stan uteslutande {\"{a}}r befattningar med h{\"{o}}ga l{\"{o}}ner som {\"{a}}r aktuella f{\"{o}}r den h{\"{a}}r typen av rekrytering, kan ett arvode uppg{\aa} till en kvarts miljon kronor. Detta kan leda till en viss selektiv varseblivning hos searchkonsulten n{\"{a}}r f{\"{o}}rtj{\"{a}}nster och brister bed{\"{o}}ms hos tillg{\"{a}}ngliga kandidater: det finns en risk att f{\"{o}}retaget f{\"{o}}resl{\aa}s tillg{\"{a}}ngliga, snarare {\"{a}}n bra, kandidater. De flesta rekryterings-och urvalssituationer ser dock annorlunda ut; oftast handlar det om medelkomplexa befattningar, utan h{\"{o}}gre formella krav, och inte s{\"{a}}llan {\"{a}}r det flera tj{\"{a}}nster som ska tills{\"{a}}ttas samtidigt. Ett exempel kan vara belysande: Ett f{\"{o}}retag s{\"{o}}ker tio nya s{\"{a}}ljare och har valt att annonsera i dagspressen f{\"{o}}r att v{\"{a}}rva s{\"{o}}kande},
author = {{Sj{\"{o}}berg, A., Sj{\"{o}}berg, S., {\&} Forss{\'{e}}n}, K},
booktitle = {PJP: Predicting Job Performance},
file = {:home/tobias/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sj{\"{o}}berg, A., Sj{\"{o}}berg, S., {\&} Forss{\'{e}}n - 2007 - Ekonomiska aspekter p{\aa} urval och arbetsprestation Rekrytering.pdf:pdf},
pages = {55--83},
title = {{Ekonomiska aspekter p{\aa} urval och arbetsprestation Rekrytering}},
year = {2007}
}
@article{Ost2017a,
abstract = {Anxiety disorders are among the most common disorders affecting youths in the general population, with up to 10{\%} of children and 20{\%} of adolescents meeting criteria for an anxiety disorder at any one point in time. Cognitive-behavior therapies (CBT), varying between 9 and 18 weeks of treatment, are considered evidence-based for the treatment of anxiety disorders in youth. During the last two decades treatments that are brief, intensive, or concentrated (BIC) have been developed and this meta-analysis includes 23 RCTs of these new approaches across the anxiety disorders. BIC yielded a lower attrition (2.3{\%}) than standard CBT (6.5{\%}). The effect sizes (ES) for comparison of BIC with waiting-list (1.47) and placebo (0.91) were significant, whereas that with standard CBT (0.01) was not. Regarding remission at post/recovery at follow-up BIC (54{\%}/64{\%}) and standard CBT (57{\%}/63{\%}) were comparable and both were significantly higher than placebo (26{\%}/35{\%}), which was higher than WLC (7{\%}/9{\%}). Within-group ES at post and follow-up were 1.50 and 1.53 for BIC, and 0.98 and 1.05 for standard CBT, indicating maintenance of the effects up to 12 months after therapy. Advantages and disadvantages of BIC are discussed and we suggest that BIC-interventions represent a paradigm shift in the delivery of services for youth with anxiety disorders.},
author = {{\"{O}}st, Lars-G{\"{o}}ran and Ollendick, Thomas H},
doi = {10.1016/j.brat.2017.07.008},
file = {:home/tobias/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/{\"{O}}st, Ollendick - 2017 - Brief, intensive and concentrated cognitive behavioral treatments for anxiety disorders in children A systemati.pdf:pdf},
isbn = {0005-7967},
issn = {1873622X},
journal = {Behaviour Research and Therapy},
keywords = {Anxiety disorders,Children,Cognitive-behavioral treatment,Meta-analysis,Systematic review},
pages = {134--145},
pmid = {28772195},
title = {{Brief, intensive and concentrated cognitive behavioral treatments for anxiety disorders in children: A systematic review and meta-analysis}},
url = {http://dx.doi.org/10.1016/j.brat.2017.07.008},
volume = {97},
year = {2017}
}
@article{Jacob2005,
abstract = {Recent advances in the cognitive neuroscience of action have considerably enlarged our understanding of human motor cognition. In particular, the activity of the mirror system, first discovered in the brain of non-human primates, provides an observer with the understanding of a perceived action by means of the motor simulation of the agent's observed movements. This discovery has raised the prospects of a motor theory of social cognition. In humans, social cognition includes the ability to mindread, and many motor theorists of social cognition try to bridge the gap between motor cognition and mindreading by endorsing a simulation account of mindreading. Here, we question the motor theory of social cognition and give reasons for our skepticism.},
author = {Jacob, Pierre and Jeannerod, Marc},
