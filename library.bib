title = {{Revealing the Cues Within Community Places: Storiesof Identity, History, and Possibility}},
url = {http://download.springer.com.ezp.sub.su.se/static/pdf/903/art{\%}253A10.1007{\%}252Fs10464-005-9006-3.pdf?originUrl=http{\%}3A{\%}2F{\%}2Flink.springer.com{\%}2Farticle{\%}2F10.1007{\%}2Fs10464-005-9006-3{\&}token2=exp=1490352930{~}acl={\%}2Fstatic{\%}2Fpdf{\%}2F903{\%}2Fart{\%}25253A10.1007{\%}25252F},
