@article{Porges1995,
abstract = {PORGES, S. W. Cardiac vagal tone: A physiological index of stress. NEUROSCI BIOBEHAV REV 19(2) 225-233, 1995.-Cardiac vagal tone is proposed as a novel index of stress and stress vulnerability in mammals. A model is described that emphasizes the role of the parasympathetic nervous system and particularly the vagus nerve in defining stress. The model details the importance of a branch of the vagus originating in the nucleus ambiguus. In mammals the nucleus ambiguus not only coordinates sucking, swallowing, and breathing, but it also regulates heart rate and vocalizations in response to stressors. In mammals it is lpossible, by quantifying the amplitude of respiratory sinus arrhythmia, to assess the tonic and phasic regulation of the vagal pathways originating in the nucleus ambiguus. Measurement of this component of vagal tone is proposed as a method to assess, on an individual basis, both stress and the vulnerability to stress. Vagal tone Stress Autonomic nervous system Heart rate Respiratory sinus arrhythmia RESEARCH on stress in adult humans has often focused on the description of events that are considered stressful (e.g., death of a spouse, loss of a job, etc) and not on the functional impact of these events on physiology. In research with adult humans, interview techniques and questionnaires are used to obtain perceptions of ewmts to determine whether they are stressful. With young pre,erbal infants and with other nonver-bal mammals, the evaluation of stress is more difficult and relies on the interpretation of behavioral and physiological reactivity. In the latter populations, the impact of stress is often observed when behavior becomes disorganized (e.g., dis-ruption in movement patterns, irregular sleep-wake patterns, difficulties in regulating behavior in social settings, extended bouts of crying, and general irritability) and homeostatic physiological processes me disrupted (e.g., difficulties in di-gestion and elimination and other problems with autonomic processes). Thus, in contrast to research with the verbal hu-man, in which stress is often treated as a psychological con-struct with a potential physiological correlate, in other mam-malian populations, stress is assumed to be a physiological construct. Unlike the behavioral and psychological emphasis in re-search with adult humans, research with other mammalian populations has focused on the assessments of behavior (e.g., crying and vocalizations) and physiology (e.g., heart rate, blood pressure, ACTH, and cortisol) in response to proce-dures that are presumed s{\~{}}essful. In the preverbal infant these procedures include circumcision (38), lumbar puncture (39), cardiac catheterization (3]L), heel lance (15), inoculation (27) and maternal separation (19). It is assumed that global mea-sures of physiological systems reflecting cardiovascular activ-ity and the status of the hypothalamic-pituitary-adrenal axis will be sensitive to the stressful events not only in infants, but also in other mammals. How these systems register stress or stress vulnerability is seldom discussed. Although there are individual differences in response thresholds (i.e., stress vulnerability) to the identical proce-dures, current definitions of stress emphasize either the treat-ment (e.g., the stressful procedure) or the global behavioral and physiological responses to treatment. They do not empha-size either the neurophysiological mechanisms mediating the response to stressors, or the uniqueness of an individual's re-sponse. The model being presented hypothesizes that neuro-physiological mechanisms promote or limit physiological reac-tivity to presumed stressful events. It is proposed that the status of neurophysiological regulation prior to exposure to a specific stressor would promote or limit physiological reactiv-ity and, thus, the physiological dimension of stress. Based upon this approach, physiological stress is functionally de-fined. The model emphasizes the role of the bralnstem in regulat-ing behavioral and physiological reactivity to stress. The model is based on neurophysiological structures, common to all mammals, that regulate autonomic processes. Specifically, in mammals the nucleus ambiguus provides the motor neu-rons: (a) to coordinate sucking, swallowing, and breathing; and (b) to regulate shifts in heart rate and vocalization fre-quencies in response to stressful events. A method will be described for assessing, on an individual basis, both the stress response and the vulnerability to stress.},
author = {Porges, Stephen W},
file = {:home/tobias/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Porges - 1995 - Cardiac Vagal Tone A Physiological Index of Stress.pdf:pdf},
isbn = {0149-7634(94)00066-2},
journal = {Neuroscience and Biobehavioral Reviews},
number = {2},
pages = {225--233},
title = {{Cardiac Vagal Tone: A Physiological Index of Stress}},
volume = {19},
year = {1995}
}
