journal = {Scandinavian journal of psychology},
keywords = {com,denmark,e-mail,gmail,kingdom,manchester m13 9wl united,metacognitive therapy,pcallesen,pia callesen,rawnsley building mri,recurrent depression,school of psychological sciences,single case study,university of manchester},
