number = {2},
pages = {116--124},
title = {{Job demands and job performance: The mediating effect of psychological and physical strain and the moderating effect of role clarity}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/1076-8998.12.2.116},
volume = {12},
year = {2007}
}
