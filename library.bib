isbn = {1364-6613},
issn = {13646613},
journal = {Trends in Cognitive Sciences},
month = {sep},
number = {9},
pages = {458--470},
pmid = {23932069},
title = {{How neurons make meaning: brain mechanisms for embodied and abstract-symbolic semantics}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1364661313001228},
volume = {17},
year = {2013}
}
