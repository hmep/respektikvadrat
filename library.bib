journal = {Clinical psychology {\&} psychotherapy},
keywords = {Adherence,Cognitive-Behavioural Therapy,Panic Disorder,Personality Disorders,Resistance},
number = {3},
pages = {272--284},
title = {{Interpersonal Factors Are Associated with Lower Therapist Adherence in Cognitive-Behavioural Therapy for Panic Disorder}},
volume = {23},
