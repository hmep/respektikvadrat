pmid = {18312121},
title = {{BPD's Interpersonal Hypersensitivity Phenotype: A Gene-Environment-Developmental Model}},
url = {https://guilfordjournals-com.ezp.sub.su.se/doi/pdf/10.1521/pedi.2008.22.1.22 http://guilfordjournals.com/doi/10.1521/pedi.2008.22.1.22},
volume = {22},
