number = {8},
pages = {1011--1023},
title = {{Current theoretical models of generalized anxiety disorder (GAD): Conceptual review and treatment implications}},
url = {http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.1009.2762{\&}rep=rep1{\&}type=pdf},
