journal = {Acta Psychiatrica Scandinavica},
keywords = {Adult,Aged,Female,Humans,Male,Middle Aged,Personality Disorders,Personality Disorders: diagnosis,Personality Disorders: epidemiology,Personality Disorders: psychology,Personality Inventory,Psychometrics,Reference Values,Reproducibility of Results,Sweden,Sweden: epidemiology},
month = {sep},
