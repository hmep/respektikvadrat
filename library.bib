keywords = {nonimprovement,outcome research,patient perspective,process research,psychoanalytic/psychodynamic therapy,qualitative research methods},
number = {5},
pages = {546--564},
pmid = {25517205},
title = {{“Spinning one's wheels”: Nonimproved patients view their psychotherapy}},
url = {http://www.tandfonline.com/action/journalInformation?journalCode=tpsr20},
volume = {25},
