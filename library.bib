publisher = {John Wiley {\&} Sons, Ltd},
title = {{Short-term psychodynamic psychotherapies for common mental disorders}},
url = {http://www.thecochranelibrary.com http://doi.wiley.com/10.1002/14651858.CD004687.pub4},
year = {2014}
}
