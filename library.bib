keywords = {Acceptance and commitment therapy,Evidence base,Meta-Analysis,Research methodology,Systematic review},
pages = {273--281},
title = {{Rebuttal of Atkins et al. (2017) critique of the {\"{O}}st (2014) meta-analysis of ACT}},
