file = {:home/tobias/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sanjeev, Surya - 2016 - Two Factor Theory of Motivation and Satisfaction An Empirical Verification.pdf:pdf},
issn = {2198-5804},
