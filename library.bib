@article{Werner-Seidler2017a,
abstract = {Depression and anxiety often emerge for the first time during youth. The school environment provides an ideal context to deliver prevention programs, with potential to offset the trajectory towards disorder. The aim of this review was to provide a comprehensive evaluation of randomised-controlled trials of psychological programs, designed to prevent depression and/or anxiety in children and adolescents delivered in school settings. Medline, PsycINFO and the Cochrane Library were systematically searched for articles published until February 2015. Eighty-one unique studies comprising 31,794 school students met inclusion criteria. Small effect sizes for both depression (g = 0.23) and anxiety (g = 0.20) prevention programs immediately post-intervention were detected. Small effects were evident after 12-month follow-up for both depression (g = 0.11) and anxiety (g = 0.13). Overall, the quality of the included studies was poor, and heterogeneity was moderate. Subgroup analyses suggested that universal depression prevention programs had smaller effect sizes at post-test relative to targeted programs. For anxiety, effect sizes were comparable for universal and targeted programs. There was some evidence that externally-delivered interventions were superior to those delivered by school staff for depression, but not anxiety. Meta-regression confirmed that targeted programs predicted larger effect sizes for the prevention of depression. These results suggest that the refinement of school-based prevention programs have the potential to reduce mental health burden and advance public health outcomes.},
author = {Werner-Seidler, Aliza and Perry, Yael and Calear, Alison L. and Newby, Jill M. and Christensen, Helen},
doi = {10.1016/j.cpr.2016.10.005},
file = {:home/tobias/Dropbox/Psykologprogrammet-tobias/Kurs25{\_}KBT/1-s2.0-S0272735815301409-main.pdf:pdf},
issn = {18737811},
journal = {Clinical Psychology Review},
keywords = {Anxiety,Depression,Meta-analysis,School-based,Systematic-review},
pages = {30--47},
publisher = {The Authors},
title = {{School-based depression and anxiety prevention programs for young people: A systematic review and meta-analysis}},
url = {http://dx.doi.org/10.1016/j.cpr.2016.10.005},
volume = {51},
year = {2017}
}
@misc{Waern2012,
abstract = {Utdrag ur ett metodkompendium av Yvonne Waern {\&} Bo Heffler 2012 Stockholms Universitet Psykologiska Institutionen 2 Inledning Att f{\"{o}}rst{\aa} hur barn t{\"{a}}nker {\"{a}}r en fr{\aa}ga v{\"{a}}rd att {\"{a}}gna ett helt kompendium. Barn ger uttryck f{\"{o}}r tankar, som f{\"{o}}r den vuxne kan te sig ologiska, orimliga och ov{\"{a}}ntade. Om vi antar att barnens f{\"{o}}rest{\"{a}}llningar om v{\"{a}}rlden skiljer sig fr{\aa}n v{\aa}ra vuxna f{\"{o}}rest{\"{a}}llningar, kan vi ocks{\aa} anta att barnens handlingar inte beh{\"{o}}ver ha samma mening f{\"{o}}r barnen som f{\"{o}}r oss. Barnens och de vuxnas v{\"{a}}rld kan ofta kollidera. Vi vuxna tycker d{\aa} ofta att det {\"{a}}r barnen som har " fel " . Emil i L{\"{o}}nneberga {\"{a}}r ett typexempel p{\aa} ett barn som ofta vill v{\"{a}}l, men som vi tycker g{\"{o}}r fel. Han hissar upp sin lillasyster i flaggst{\aa}ngen f{\"{o}}r att hon ska f{\aa} se {\"{a}}nda bort till n{\"{a}}sta socken – vilken generositet, n{\"{a}}r han antagligen helst sj{\"{a}}lv hade velat sv{\"{a}}va d{\"{a}}r uppe! Hur litet sj{\"{a}}lvklar v{\"{a}}rlden {\"{a}}r, visar oss barnen med sina fr{\aa}gor. " Varf{\"{o}}r kl{\"{a}}ttrar inte r{\aa}ttor i tr{\"{a}}d? " " Var tar himlen egentligen slut? " Ett s{\"{a}}tt att f{\"{o}}rs{\"{o}}ka f{\"{o}}rst{\aa} barn {\"{a}}r allts{\aa} att umg{\aa}s med dem. " G{\aa} med dina barn p{\aa} en fj{\"{a}}llvandring i n{\aa}gra dagar, d{\aa} f{\aa}r du h{\"{o}}ra vad barnen t{\"{a}}nker p{\aa} " s{\"{a}}ger den danske familjeterapeuten Jesper Juul (2012). F{\"{o}}r att l{\"{a}}ra oss n{\aa}got om och av barnen m{\aa}ste vi l{\"{a}}ra oss att se och att lyssna. Det {\"{a}}r alltf{\"{o}}r l{\"{a}}tt att {\"{o}}versk{\"{o}}lja barnen med v{\aa}r vuxna v{\"{a}}rld, och av barnen bara f{\aa} tillbaka en blek kopia av oss sj{\"{a}}lva. Det {\"{a}}r ocks{\aa} l{\"{a}}tt att av barns handlingar och uttalanden l{\"{a}}sa ut den konventionella vuxna f{\"{o}}rest{\"{a}}llningsv{\"{a}}rlden