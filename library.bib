journal = {Frontiers in Psychology},
keywords = {associative processing,context,object recognition,perception,predictions,priming,top-down,visual awareness},
title = {{Predictive Feedback and Conscious Visual Experience}},
url = {http://journal.frontiersin.org/article/10.3389/fpsyg.2012.00620/abstract},
volume = {3},
