file = {:home/tobias/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Cacioppo et al. - 2002 - Loneliness and Health Potential Mechanisms.pdf:pdf},
isbn = {0033-3174 (Print)$\backslash$r0033-3174 (Linking)},
issn = {0033-3174},
journal = {Psychosomatic Medicine},
keywords = {aging,blood pressure,cardiovascular activation,co ϭ cardiac output,cortisol,dbp ϭ diastolic blood,hr ϭ heart rate,loneliness,pep ϭ preejection period,pressure,resis-,sbp ϭ systolic blood,tpr ϭ total peripheral},
month = {may},
number = {3},
