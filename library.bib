keywords = {Affective stimuli,Attention,Cognition,Depression,Emotion,Humans,Individual differences,Individuality,Inhibition (Psychology),Memory,Models,Personality,Psychological,Short-Term,Task demands,Valence},
number = {1},
pages = {224--237},
title = {{Antecedent- and response-focused emotion regulation: Divergent consequences for experience, expression, and physiology.}},
volume = {74},
year = {1998}
}
