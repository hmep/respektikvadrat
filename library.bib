@phdthesis{Deak2015a,
abstract = {Specifik fobi {\"{a}}r en vanlig psykiatrisk {\aa}komma som kan leda till stora individuella begr{\"{a}}nsningar. Symtomen kan framg{\aa}ngsrikt behandlas med kognitiv beteendeterapi d{\"{a}}r 85–90 {\%} blir kliniskt signifikant f{\"{o}}r-b{\"{a}}ttrade. Forskning p{\aa}visar lovande behandlingsutfall f{\"{o}}r virtuell ex-poneringsbehandling (VRET) mot spindelfobi. Tekniken {\"{a}}r intressant d{\aa} den kringg{\aa}r de problem med anskaffning och f{\"{o}}rvaring av fobiska stimuli som sedvanlig behandling medf{\"{o}}r och dessutom kan inneb{\"{a}}ra {\"{o}}kad tillg{\"{a}}nglighet och flexibilitet vid behandling. Syftet med f{\"{o}}relig-gande studie {\"{a}}r att j{\"{a}}mf{\"{o}}ra behandlingseffekten av ensessionsbehand-ling (OST) med en nyutvecklad spelifierad virtuell exponeringsbe-handling (VIMSE), som sker under en frist{\aa}ende behandlingssession. Totalt randomiserades 73 deltagare mellan de tv{\aa} behandlingsmeto-derna. B{\aa}da behandlingarna medf{\"{o}}rde statistiskt signifikanta f{\"{o}}rb{\"{a}}tt-ringar med stora effektstorlekar f{\"{o}}r s{\aa}v{\"{a}}l det beteendetest (BAT), som utgjorde det prim{\"{a}}ra utfallsm{\aa}ttet (OST d = 1,94; VIMSE d = 1,41), som f{\"{o}}r de sekund{\"{a}}ra utfallsm{\aa}tten Spider Phobia Questionnaire och Fear of Spiders Questionnaire. OST resulterade i signifikant fler kli-niskt signifikant f{\"{o}}rb{\"{a}}ttrade {\"{a}}n VIMSE. Specifik fobi definieras enligt Diagnostic and Statistical Manual of Mental Disorders (American Psychiatric Association, 2013) som en p{\aa}taglig r{\"{a}}dsla f{\"{o}}r ett specifikt objekt eller en specifik situation. R{\"{a}}dslan f{\"{o}}r dessa objekt eller situationer {\"{a}}r orimlig och resul-terar i att individen undviker att konfronteras med den stimulus som v{\"{a}}cker r{\"{a}}dslan eller uth{\"{a}}rdar dess n{\"{a}}rvaro under stort lidande eller flyr. Konsekvenserna av specifik fobi kan omfatta stora neds{\"{a}}ttningar i individens funktionsf{\"{o}}rm{\aa}ga, s{\aa}som sv{\aa}righeter att s{\"{o}}ka v{\aa}rd, problem att delta i sociala aktiviteter eller minskad f{\"{o}}rm{\aa}ga att sk{\"{o}}ta sitt arbete eller sina studier (Davis, Ollendick, {\&} {\"{O}}st, 2012). DSM-5 anger fem huvudkategorier av specifik fobi: levande varelser, naturf{\"{o}}reteelser, blod-sprutor-skador, situationer samt andra former (American Psychiatric Association, 2013) och av dessa {\"{a}}r fobi mot djur vanligast (Becker et al., 2007). Prevalensen {\"{a}}r h{\"{o}}g i samtliga {\aa}lderskohorter (Fredrikson, Annas, Fischer, {\&} Wik, 1996; King, Eleonora, {\&} Ollendick, 1998; Sigstr{\"{o}}m et al., 2011). En analys som bygger p{\aa} data fr{\aa}n The National Comorbidity Survey Replication (NCS-R) med 9282 respondenter estimerade livstidsprevalensen f{\"{o}}r specifik fobi till 15,6 {\%} (Kessler et al., 2012). Subkliniska besv{\"{a}}r {\"{a}}r vanliga och upp till 1 Vi vill rikta ett varmt tack till v{\aa}r handledare Per Carlbring, som ocks{\aa} varit ansvarig f{\"{o}}r denna studie. Vi vill {\"{a}}ven tacka Philip Lindner (behandlingssamordnare), Lena Reuterski{\"{o}}ld (klinisk handledare), Katherine Cotter (forskningsassistent) och Alexander (Spiderman) Miloff fr{\aa}n Stockholms Universitet; T{\"{o}}rnhage och Maria Zenger. Utan alla ni fantastiska, duktiga, engagerade och hj{\"{a}}lpsamma personer hade detta examensarbete aldrig kunnat f{\"{o}}rverkligas.},
author = {Deak, Stefan and Kristoffersson, Glenn},
file = {:home/tobias/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Deak, Kristoffersson - 2015 - R{\"{a}}dslan f{\"{o}}r det som finns och inte finns.pdf:pdf},
title = {{R{\"{a}}dslan f{\"{o}}r det som finns och inte finns}},
year = {2015}
}
