title = {{Standardization of the rathus assertiveness schedule}},
volume = {33},
year = {1977}
}
@article{Sroufe2003,
abstract = {The papers presented in this issue represent efforts to understand the effects of extreme experiences on the attachment system and the individuals therein, including second and third generation effects. In work carried out in England, Israel, Sweden, and the US, authors examined the effects of extreme trauma on elderly survivors of the holocaust; on children who have witnessed brutal violence against a parent; on those who have experienced ritual violence at the hands of family members; and on at-risk children who have experienced losses or abuse. One cannot read these stories without thinking about the effects of war or other disasters on human beings. It becomes clear that experiences such as these can be incomprehensible to those caught up in them, when they are incomprehensible to us in reading about them. Closer to home, if children are raised in situations that are so compromising of healthy development, that as adults, they are unable to cooperate, compromise, understand and forgive, our future is clouded. We appreciate the courage of these individuals to tell us their stories, and we appreciate the commitment of these researchers to help us all understand and learn from them. (PsycINFO Database Record (c) 2012 APA, all rights reserved).},
author = {Sroufe, June},
doi = {10.1080/14616730310001633401},
file = {:home/tobias/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sroufe - 2003 - Commentary Comprehending the incomprehensible.pdf:pdf},
issn = {1461-6734},
journal = {Attachment {\&} Human Development},
month = {dec},
