doi = {10.1177/0013164406288171},
file = {:home/tobias/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Scherbaum, Cohen-Charash, Kern - 2006 - Measuring General Self-Efficacy A Comparison of Three Measures Using Item Response Theory.pdf:pdf},
