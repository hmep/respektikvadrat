journal = {Behaviour Research and Therapy},
keywords = {Cognitive skills,Depression,Prevention intervention,Rumination,Self-help},
month = {feb},
number = {2},
pages = {152--157},
title = {{When self-help is no help: Traditional cognitive skills training does not prevent depressive symptoms in people who ruminate}},
url = {http://ac.els-cdn.com.ezp.sub.su.se/S0005796709002307/1-s2.0-S0005796709002307-main.pdf?{\_}tid=dcb00ca8-825c-11e7-977c-00000aacb35f{\&}acdnat=1502872281{\_}5ea58d169fe0b88718d3c3f41b2c9c24 http://linkinghub.elsevier.com/retrieve/pii/S0005796709002307},
