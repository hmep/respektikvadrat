isbn = {1086-3303},
issn = {1086-3303},
journal = {Philosophy, Psychiatry, {\&} Psychology},
keywords = {2009 by the johns,Charcot effect,Efficient cause,charcot effect,clinical,clinical psychology,efficient cause,final cause,formal cause,hopkins university press,material cause,psychiatry,psychology,social drama},
number = {3},
pages = {211--225},
title = {{More Aristotle, Less DSM: The Ontology of Mental Disorders in Constructivist Perspective}},
