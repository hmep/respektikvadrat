journal = {Behavior Therapy},
keywords = {Acceptance-based behavior therapy,Anxiety,Depression,Long-term follow-up,Psychotherapy outcome},
number = {4},
pages = {801--811},
publisher = {Elsevier B.V.},
title = {{Long-Term Follow-Up of a Randomized Controlled Trial Comparing Acceptance and Commitment Therapy and Standard Cognitive Behavior Therapy for Anxiety and Depression}},
url = {http://dx.doi.org/10.1016/j.beth.2012.04.004},
volume = {43},
