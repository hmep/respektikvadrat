title = {{Partially overlapping sensorimotor networks underlie speech praxis and verbal short-term memory: evidence from apraxia of speech following acute stroke}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4142788{\&}tool=pmcentrez{\&}rendertype=abstract http://journal.frontiersin.org/article/10.3389/fnhum.2014.00649/abstract},
volume = {8},
